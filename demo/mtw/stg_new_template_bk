    <MTIgnore><mt:If name="signup_form" _note="MTW_MEMBERS-340"></MTIgnore>
    <script>
    $(function(){
        $('.checkbox-selector').each(function(){
            var hide_next_all_checkbox = true;
            var $cb_selector = $(this);
            var $next_all_checkbox = $cb_selector.nextAll();
            if($next_all_checkbox.find('input:checked').length){
                hide_next_all_checkbox = false;
            }
            if(hide_next_all_checkbox){
                $next_all_checkbox.hide();
                $cb_selector.find('a').on('click',function(){
                    var $button = $(this);
                    $button.parent().nextAll().show();
                    $button.parent().remove();
                    return false;
                });
            }else{
                $cb_selector.remove();
            }
        });
        $('.radio-selector').each(function(){
            var hide_radio = true;
            var $r_selector = $(this);
            var $radios = $r_selector.siblings('.radio-selector-hide');
            if($radios.find('input:checked').length){
                hide_radio = false;
            }
            if(hide_radio){
                $radios.hide();
                $r_selector.find('a').on('click',function(){
                    var $button = $(this);
                    $button.parent().siblings('.radio-selector-hide').show()
                        .insertBefore($button.parent());
                    $button.parent().remove();
                    return false;
                });
            }else{
                $r_selector.remove();
            }
        });
        // 「主たる診療領域」の並び順
        var sort_order = [
            '一般内科',
            '外科',
            '乳腺外科',
            '消化器内科',
            '循環器内科',
            '小児科',
            '整形外科',
            '呼吸器内科',
            '精神・神経科',
            '消化器外科'
        ];
        var $category_main = $('input[name=category_main]');
        for(var i = sort_order.length - 1; i >= 0; i--){
            var $label = $category_main.closest('label[data-name="' + sort_order[i] + '"]');
            if($label.length){
                $label.prependTo($label.parent());
            }
        }
    });
    </script>
    <MTIgnore></mt:If></MTIgnore>

		<script>
			// 入力欄の非表示
			(function(){
				// 非表示にするブロックのclass名を指定する
				var target_eles = [
					<mt:If name="use_emp_selector">
					'#empname_1',
					'.empname_1_text',
					'.col-employment',
					</mt:If>
					// '.col-regyear',
					'.col-graduniversity',
					//'.col-mainemplcategory',
					'.col-classification',
					<mt:If name="signup_form">
					'.add-other-work',
					</mt:If>
					'.col-empltel',
					'.col-affiliation',
					'.col-etcemplcategory'
					<MTIgnore>MTW_MEMBERS-741 MTW本人確認機能の実装</MTIgnore>
					<mt:Unless name="signup_form">
					,'.col-sex'
					,'.col-birthday'
					</mt:Unless>
				];
				$('form')
					.find(target_eles.join(','))
					.addClass('__sf-hide_block');
				
				
				<mt:If name="use_emp_selector">
				// #empname_1のみ位置を調整する
				$('#empname_1').each(function(){
					var $empname   = $(this);
					var $container = $empname.closest('.r');
					var $selector  = $container.find('.empname_selector-box');
					$empname.css('top','-' + $selector.outerHeight(true) + 'px');
				});
				</mt:If>
				
			})();
			
			// 項目のダミー入力
			(function(){
				var dummy_data = [
					/*
					{
						name  : 'customfield_licenseyear__c', // 医師登録年度
						value : '9999'
					},*/
					{
						name  : 'customfield_mstuniversityid__c', // 出身大学名
						label : '9999' // labelを指定した場合は value ではなくラベルで選択する
					},{
						name  : 'class_1', // 勤務先区分（医師）
						value : '9999'
					},{
						name  : 'tel_1', // 主たる勤務先→勤務先電話番号
						value : '9999999999'
					}
				];
				for(var i = 0; i < dummy_data.length; i++){
					var name = dummy_data[i].name;
					var dummy_value = dummy_data[i].value;
					var dummy_label = dummy_data[i].label;
					if(!name){ continue; };
					var $input = $('[name="' + name + '"]');
					if(!$input.length){ continue; };
					if($input.val()){ continue; };
					
					var tag  = $input.eq(0)[0].tagName.toUpperCase(); // タグ名
					var type = $input.eq(0).attr('type'); // type属性値
					if(type){ type = type.toUpperCase() };
					
					if(tag == 'SELECT'){
						$input.find('option:selected').prop('selected',false).removeAttr('selected');
						// マッチする要素があればそれを選択する・無ければ作る
						var is_matched = false;
						if(dummy_value && !is_matched){
							$input.find('option[value=' + dummy_value + ']').each(function(){
								$(this).prop('selected',true);
								is_matched = true;
							});
						}
						if(dummy_label && !is_matched){
							$input.find('option:contains(' + dummy_label + ')').each(function(){
								if($(this).html() == dummy_label){
									$(this).prop('selected',true);
									is_matched = true;
								}
							});
						}
						if(!is_matched){
							$input.append('<option value="' + dummy_value + '" selected>' + dummy_value + '</option>');
						}
					}else
					if(tag == 'INPUT' && (type == 'RADIO' || type == 'CHECKBOX')){
						var $wrap = $('<label>')
							.html('&nbsp;' + dummy_value);
						var $input_new = $('<input>')
							.attr({
								type	: type.toLowerCase(),
								name	: name,
								checked : 'checked'
							})
							.val('')
							.prependTo($wrap);
						$input.eq(0).closest('label').before($wrap);
					}else{
						$input.val(dummy_value);
					}
				}
			})();
			
			<mt:Unless name="signup_form">
			// 所属/専門を自動選択する
			(function(){
				var $parent = $('.col-affiliation');
				var $check1 = $('input[data-label="認定医・専門医"]',$parent);
				var $check2 = $('input[data-label="所属している学会"]',$parent);
				
				if(!$check1.filter(':checked').length){
					$check1.filter(':first').prop('checked',true);
				}
				if(!$check2.filter(':checked').length){
					$check2.filter(':first').prop('checked',true);
				}
			})();
			</mt:Unless>
			
			// 施設名の選択入力
			(function(){
				var target_keys = [
					{
						name : '#empname_1', // 「施設名」入力欄ID
						rel  : {
							code : '#shisetsu_code__c', // 「施設コード」欄ID
							zip  : '#zip_1',     // 「郵便番号」入力欄ID
							pref : '#pref_1',    // 「都道府県」入力欄ID
							city : '#city_1',    // 「市区町村」入力欄ID
							addr : '#address_1', // 「番地・建物名」入力欄ID
							tel  : '#tel_1'      // 「電話番号」入力欄ID
						},
						main_block : '.col-mainemplplace', // 「勤務先施設名」ブロックのclass名
						address_block : [
							'.col-empladdress1', // 「郵便番号」入力欄を内包するブロックのclass名
							'.col-empladdress2', // 「都道府県」入力欄を内包するブロックのclass名
							'.col-empladdress3', // 「市区町村」入力欄を内包するブロックのclass名
							'.col-empladdress4', // 「番地・建物名」入力欄を内包するブロックのclass名
						],
						dummy : {
							tel: '9999999999'    // 「電話番号」が空の場合のダミー値
						}
					}
				];
						
				// JSONからプルダウンを作る関数
				var build_pulldown = function(args){
					var json    = args.json;
					var selects = args.selects;
					var type    = args.type;
					if(json.result && json.result.length){
						if(type == 'pref'){
							var option_html = '';
							for(var i = 0; i < json.result.length; i++){
								var d = json.result[i];
								option_html += '<option value="' + d.city_number + '"';
								$.each(d,function(key,val){
									option_html += ' data-' + key + '="' + val + '"';
								});
								option_html += '>' + d.displayname + '</option>';
							};
							selects.$select_city.append(option_html);
						}else
						if(type == 'city'){
							var option_html = '';
							for(var i = 0; i < json.result.length; i++){
								var d = json.result[i];
								option_html += '<option value="' + d + '"';
								option_html += '>' + d + '</option>';
							};
							selects.$select_town.append(option_html);
						}else
						if(type == 'town'){
							var option_html = '';
							for(var i = 0; i < json.result.length; i++){
								var d = json.result[i];
								option_html += '<option value="' + d.displayname + '"';
								$.each(d,function(key,val){
									if(val){
										option_html += ' data-' + key + '="' + val + '"';
									}
								});
								option_html += '>' + d.displayname + '（' + d.address_1 + '）' + '</option>';
							};
							selects.$select_name.append(option_html);
						}
					}
				}
				
				// <select>の選択動作
				var select_event = function(event,callbacks){
					var $select = $(this);
					var keys    = $select.data('keys');
					var selects = $select.data('selects');
					var type    = $select.data('type');
					var target  = $select.data('target');
					
					var $target      = selects.$target;
					var $select_pref = selects.$select_pref;
					var $select_city = selects.$select_city;
					var $select_town = selects.$select_town;
					var $select_name = selects.$select_name;
					var $select_data = selects.$select_data;
					
					var val      = $select.val();
					var val_pref = $select_pref.val();
					var val_city = $select_city.val();
					var val_town = $select_town.val();
					var val_name = $select_name.val();
					
					if(!val){
						// 施設名と住所情報をクリアする
						$target.val('');
						$.each(keys.rel,function(_key,_val){
							$(_val).val('');
						});
						$(keys.rel.tel).val(keys.dummy.tel);
						
						if(type == 'pref'){
							$select_city.val('').find('option').not('.inq').remove();
							$select_town.val('').find('option').not('.inq').remove();
							$select_name.val('').find('option').not('.inq').remove();
						}else
						if(type == 'city'){
							$select_town.val('').find('option').not('.inq').remove();
							$select_name.val('').find('option').not('.inq').remove();
						}else
						if(type == 'town'){
							$select_name.val('').find('option').not('.inq').remove();
						}
						
						if(type == 'pref' || type == 'city' || type == 'town'){
							return false;
						}
					}
					
					if(type == 'pref' || type == 'city' || type == 'town'){
						// 送信パラメータの設定
						var url = '';
						var params = {};
						if(type == 'pref'){
							params = {
								__mode: 'city_list',
								pref_code : val_pref
							}
						}else
						if(type == 'city'){
							params = {
								__mode: 'town_list',
								pref_code: val_pref,
								city_number : val_city
							}
						}else
						if(type == 'town'){
							params = {
								__mode: 'hosp_list',
								pref_code: val_pref,
								city_number : val_city,
								town: val_town,
								sort_by: 'displayname'
							}
						}
						
						// 施設名と住所情報をクリアする
						$target.val('');
						$.each(keys.rel,function(_key,_val){
							$(_val).val('');
						});
						$(keys.rel.tel).val(keys.dummy.tel);
						
						// ajax でデータを取得する
						$.ajax({
							url: '/prefcity.cgi',
							data: params,
							dataType: 'json',
							_type: type,
							_target : $target,
							_selects: {
								$select_pref : $select_pref,
								$select_city : $select_city,
								$select_town : $select_town,
								$select_name : $select_name,
								$select_data : $select_data
							},
							beforeSend: function(){
								var selects = this._selects;
								var type = this._type;
								
								// 最初のoption以外削除する
								if(type == 'pref'){
									selects.$select_city.val('').find('option').not('.inq').remove();
									selects.$select_town.val('').find('option').not('.inq').remove();
									selects.$select_name.val('').find('option').not('.inq').remove();
								}else
								if(type == 'city'){
									selects.$select_town.val('').find('option').not('.inq').remove();
									selects.$select_name.val('').find('option').not('.inq').remove();
								}else
								if(type == 'town'){
									selects.$select_name.val('').find('option').not('.inq').remove();
								}
								$.each(selects,function(){
									this
										.prop('disabled',true)
										.css('opacity','0.5')
										.find('option:selected').each(function(){
											$(this).text($(this).text() + ' (読み込み中...)');
										});
								});
								// 勤務先施設を「一覧から選択」にした状態で change を発火させて住所欄を消す
								selects.$select_name.val('').trigger('change');
							},
							complete: function(){
								var selects = this._selects;
								var type = this._type;
								$.each(selects,function(){
									this
										.prop('disabled',false)
										.css('opacity','')
										.find('option:selected').each(function(){
											$(this).text($(this).text().replace(/ \(読み込み中\.\.\.\)/g,''));
										});
								});
							},
							success: function(json){
								build_pulldown({
									json    : json,
									selects : this._selects,
									type    : this._type
								});
								if(callbacks && callbacks.success){
									callbacks.success();
								}
							},
							error: function(XMLHttpRequest, textStatus, errorThrown){
								var selects = this._selects;
								var type = this._type;
								if(callbacks && callbacks.error){
									callbacks.error();
								}
							}
						});
					}else
					if(type == 'name'){
						// 勤務先施設の選択
						var $main_block = $(keys.main_block);
						var $address_block = $main_block.siblings('.__sf-address_block');
						$address_block.addClass('__sf-hide_block');
						$target.addClass('__sf-hide_block');
                        if($('.empname_1_text').length) $('.empname_1_text').addClass('__sf-hide_block'); 
						if(val == '__FORCE_INPUT__'){
                            if($('.empname_1_text').length) $('.empname_1_text').removeClass('__sf-hide_block'); 
							$target.removeClass('__sf-hide_block');
							if(!$address_block.length){
								$address_block = $('<div>')
									.addClass('__sf-address_block')
									.insertAfter($main_block);
								var $address = $('form').find(keys.address_block.join(','));
								if($address.length){
									$address.appendTo($address_block);
								}
							}else{
								$address_block.removeClass('__sf-hide_block');
								// return true;
							}
							$(keys.rel.tel).val(keys.dummy.tel);
							$(keys.rel.code).val('');
						}else
						if(val){
						
							// 施設名と住所情報をクリアする
							$target.val('');
							$.each(keys.rel,function(_key,_val){
								$(_val).val('');
							});
							
							var $option = $select_name.find('option:selected');
							
							// 施設名をセットする
							$target.val(val);
							
							// 住所をセットする
							// 郵便番号
							var data_zip = $option.attr('data-zip').replace(/\-/g,'');
							$(keys.rel.zip).val(data_zip);
							
							// 住所
							var address   = $option.attr('data-address_1');
							/*
							if($option.attr('data-address_2')){
								address = address + $option.attr('data-address_2');
							}
							*/
							var data_pref = $select_pref.find('option:selected').text(); // 都道府県
							// var data_city = address.replace(data_pref,'').replace(/(.+?)([0-9０-９]+.*)/,'$1'); // 市区町村
							var data_city = $select_city.find('option:selected').text() + $select_town.find('option:selected').text(); // 市区町村
							// var data_addr = address.replace(/(.+?)([0-9０-９]+.*)/,'$2'); // 番地・建物名
							var data_addr = address.replace(data_pref + data_city,'');
							if(!data_addr){
								data_addr = 'なし';
							}
							
							$(keys.rel.pref).val(data_pref);
							$(keys.rel.city).val(data_city);
							$(keys.rel.addr).val(data_addr);
							
							// 電話番号
							var data_tel = $option.attr('data-tel'); 
							if(!data_tel){
								data_tel = keys.dummy.tel;
							}else{
								data_tel = data_tel.replace(/\-/g,'');
							}
							$(keys.rel.tel).val(data_tel);
							
							// 施設コード
							var data_code = $option.attr('data-code');
							$(keys.rel.code).val(data_code);
						}
						
						// 「戻る」「リロード」対応
						var option_data = {
							pref : $select_pref.val(),
							city : $select_city.val(),
							town : $select_town.val(),
							name : $select_name.val()
						}
						$select_data.val(JSON.stringify(option_data));
					}
				}
				
				for(var z = 0; z < target_keys.length; z++){
					var keys     = target_keys[z];
					var target  = keys.name;
					
					var $select_pref = $(target + '-select-pref');
					var $select_city = $(target + '-select-city');
					var $select_town = $(target + '-select-town');
					var $select_name = $(target + '-select-name');
					var $select_data = $(target + '-select-data');
					
					if(!$select_pref.length){
						continue;
					}
					
					var eles = {
						$target      : $(target),
						$select_pref : $select_pref,
						$select_city : $select_city,
						$select_town : $select_town,
						$select_name : $select_name,
						$select_data : $select_data
					}
					
					$select_pref.data('keys',keys).data('selects',eles).on('change',select_event).val('');
					$select_city.data('keys',keys).data('selects',eles).on('change',select_event).val('');
					$select_town.data('keys',keys).data('selects',eles).on('change',select_event).val('');
					$select_name.data('keys',keys).data('selects',eles).on('change',select_event).val('');
					
					// 「戻る」「更新」時のプルダウンの復元
					try{
						var select_data_val = $select_data.val();
						if(select_data_val){
							var selected = JSON.parse(select_data_val);
							
							if(selected['name'] == '__FORCE_INPUT__'){
								$select_name
									.val(selected['name'])
									.trigger('change');
							}else{
								// 都道府県
								if(selected['pref']){
									$select_pref
										.val(selected['pref'])
										.trigger('change');
								}
								// 市区町村
								if(selected['city']){
									var timer_city = setInterval(function(){
										if($select_city.find('option').not('.inq').length){
											clearInterval(timer_city);
											$select_city
												.val(selected['city'])
												.trigger('change');
										}
									},100);
								}
								// 市区町村以下
								if(selected['town']){
									var timer_town = setInterval(function(){
										if($select_town.find('option').not('.inq').length){
											clearInterval(timer_town);
											$select_town
												.val(selected['town'])
												.trigger('change');
										}
									},100);
								}
								// 施設名
								if(selected['name']){
									var timer_name = setInterval(function(){
										if($select_name.find('option').not('.inq').length){
											clearInterval(timer_name);
											$select_name
												.val(selected['name'])
												.trigger('change');
										}
									},100);
								}
							}
						}else{
							<mt:Unless name="signup_form">
							<mt:If name="address_checked">
								// 既存ユーザー編集画面を開いた場合
								var default_pref = $select_pref.data('value');
								var default_city = $select_city.data('value'); // 「市区町村」+「市区町村以下」のため注意
								var default_town = $select_town.data('value'); // 「市区町村」+「市区町村以下」のため注意
								var default_name = $select_name.data('value');
								// 上から順番に逆引する
								if(default_pref && default_city && default_town && default_name){
									
									var error_action = function(){
										$select_pref.val('');
										$select_city.val('');
										$select_town.val('');
										$select_name.val('__FORCE_INPUT__').trigger('change');
										
										// 初期値に戻す
										var $input;
										$input = $(keys.name);
										$input.val($input.data('value'));
										for (key in keys.rel) {
											$input = $(keys.rel[key]);
											$input.val($input.data('value'));
										}
									}
									
									// 都道府県の検索
									$select_pref
										.find('option:contains('+ default_pref +')')
										.prop('selected',true)
										.end()
										.trigger('change',{
											success: function(){
												// 市区町村の検索
												var _opts = [];
												$select_city.find('option').each(function(){
													var $option = $(this);
													var val = $option.attr('data-displayname');
													if(val){
														// 「市区町村」+「市区町村以下」のため前方一致で調べる
														var reg = new RegExp('^' + val);
														if(reg.test(default_city)){
															var data = {
																$option : $option,
																val     : val,
																count   : val.length
															}
															// 2つ以上ある場合を考慮
															if(_opts[0] && (_opts[0]['count'] < val.length)){
																_opts.unshift(data);
															}else{
																_opts.push(data);
															}
														}
													}
												});
												if(_opts.length){
													// 2つ以上ある場合を考慮
													_opts[0]['$option'].prop('selected',true);
												}
												
												if($select_city.val()){
													$select_city.trigger('change',{
														success: function(){
															// 市区町村以下の検索
															var _opts = [];
															$select_town.find('option').each(function(){
																var $option = $(this);
																var val = $option.attr('value');
																if(val){
																	// 「市区町村」+「市区町村以下」のため後方一致で調べる
																	var reg = new RegExp(val + '$');
																	if(reg.test(default_town)){
																		var data = {
																			$option : $option,
																			val     : val,
																			count   : val.length
																		}
																		// 2つ以上ある場合を考慮
																		if(_opts[0] && (_opts[0]['count'] < val.length)){
																			_opts.unshift(data);
																		}else{
																			_opts.push(data);
																		}
																	}
																}
															});
															if(_opts.length){
																// 2つ以上ある場合を考慮
																_opts[0]['$option'].prop('selected',true);
															}
															if($select_town.val()){
																$select_town.trigger('change',{
																	success: function(){
																		// 勤務先施設名の確認
																		$select_name.find('option').each(function(){
																			var $option = $(this);
																			var val = $option.attr('value');
																			if(val == default_name){
																				$option.prop('selected',true);
																			}
																		});
																		if($select_name.val()){
																			$select_name.trigger('change');
																		}else{
																			error_action();
																		}
																	},
																	error: function(){
																		error_action();
																	}
																});
															}else{
																error_action();
															}
														},
														error: function(){
															error_action();
														}
													});
												}else{
													error_action();
												}
											},
											error: function(){
												error_action();
											}
										});
								}
							</mt:If>
							</mt:Unless>
						}
					}catch(e){
						
					}
					<mt:Unless name="signup_form">
					<mt:Unless name="address_checked">
						var select_data_val = $select_data.val();
						if(!select_data_val){
							$select_name.trigger('change');
						}
					</mt:Unless>
					</mt:Unless>
				}
			})();
		</script>
	</mt:If>
	<MTIgnore>MTW_MEMBERS-368</MTIgnore>
	<mt:If test="$target == 1">
		<script>
			$(function(){
				// 役職の表示制御
				$('#customfield_mstpositionid__c').on('change',function(e,p){
					var $selected  = $(this).find('option:selected');
					var $etc_input = $('#customfield_positionnote__c');
					var $etc_wrap  = $etc_input.parent();
					$etc_wrap.hide();
					if(!p){
						$etc_input.val('');
					}
					if($selected.hasClass('option-toggle')){
						$etc_wrap.show();
						if(!p){
							$etc_input.focus();
						}
					}
				}).trigger('change',['load']);
			});
		</script>
	</mt:If>
	<MTIgnore>/MTW_MEMBERS-368</MTIgnore>
	
	<MTIgnore>MTW_MEMBERS-372 転職支援サービス</MTIgnore>
	<MTIgnore>
    <mt:If test="$target == 1">
		<script>
			$(function(){
				// 転職支援サービス 表示制御
				var $radio = $('input[name="customfield_jcflag__c"]');
				var toggle_input = function(e){
					// this は ラジオボタン
					var $this = $(this);
					var $parent = $this.closest('.right');
					var $siblings = $parent.siblings('.right');
					if($this.val() == '1'){
						$siblings.insertAfter($parent).show(); // CSS（下線を付与）を考慮して並べ替える
					}else{
						$siblings.insertBefore($parent).hide(); // CSS（下線を付与）を考慮して並べ替える
					}
				}
				$radio.on('ifChanged',toggle_input);
				
				if($radio.filter(':checked').length){
					$radio.filter(':checked').trigger('ifChanged');
				}else{
					toggle_input.call($radio.filter('[value=""]')[0]);
				}
				// チェックボックスの処理（選択項目を「;」区切りで保存するようにする）
				var $checkbox = $('input[data-checkbox-to]');
				$checkbox.on('ifChanged',function(){
					var $this = $(this);
					var name = $this.attr('data-checkbox-to');
					if(name){
						var values = [];
						$('input[data-checkbox-to="' + name + '"]:checked').each(function(){
							values.push($(this).val());
						});
						$('input[name="' + name + '"]').val(values.join(';'));
					}
				});
			});
		</script>
	</mt:If>
	
    <mt:If name="hide_job_change">
    <style scoped>
    .col-job_change {
        display: none !important;
    }
    </style>
    </mt:If>
    </MTIgnore>
	<MTIgnore>/MTW_MEMBERS-372 転職支援サービス</MTIgnore>
	
</mt:If>

</MTSalesforceMTWSetAccountVars>

</form>

</mt:If>
